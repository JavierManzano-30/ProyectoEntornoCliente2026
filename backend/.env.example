PORT=3001
NODE_ENV=development
JWT_SECRET=adaitssecretkey

# Recommended: use Supabase Postgres connection string for every SQL module.
# Example format:
# DATABASE_URL=postgresql://postgres.<project-ref>:<password>@aws-0-<region>.pooler.supabase.com:6543/postgres
DATABASE_URL=

# Supabase client (used by modules that query through supabase-js)
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# Legacy pg fields (optional fallback if DATABASE_URL is not set)
DB_HOST=
DB_PORT=5432
DB_USER=
DB_PASSWORD=
DB_NAME=
DB_SSL=true
